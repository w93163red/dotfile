{
    "coc.preferences.formatOnType": true,
    "coc.preferences.formatOnSaveFiletypes": ["python", "rust", "css", "javascript", "typescript", "typescriptreact", "json", "javascriptreact", "typescript.tsx"],
    "rust-analyzer.lens.run": true,
    "rust-analyzer.lens.enable": true,
    "rust-analyzer.inlayHints.refreshOnInsertMode": true,
    "rust-analyzer.assist.importMergeBehaviour": "full",
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "rust-analyzer.rustfmt.extraArgs": [
        "--edition",
        "2018"
    ],
    "rust-analyzer.callInfo.full": true,
    "rust-analyzer.checkOnSave.enable": false,
    "rust-analyzer.completion.addCallParenthesis": true,
    "rust-analyzer.procMacro.enable": true, 
    "rust-analyzer.cargo.loadOutDirsFromCheck": false,
    "rust-analyzer.inlayHints.chainingHints": true,
    "rust-analyzer.hoverActions.enable": true,
    "explorer.buffer.showHiddenBuffers": true,
    "explorer.icon.enableNerdfont": true
}
